// for(i=0; i<k; i++)
function findMax() {
    var max = 0;
    for (var i = 0; i < arguments.length; i++) {
        if (arguments[i] > max) max = arguments[i];
    }
    return max;
}
$("#p1").html(findMax(4, 5, 6));

// for(index in array)
function findMax2() {
    var max = 0;
    for (var index in arguments) {
        if (arguments[index] > max) max = arguments[index];
    }
    return max;
}
$("#p2").html(findMax2(4, 5, 8));

